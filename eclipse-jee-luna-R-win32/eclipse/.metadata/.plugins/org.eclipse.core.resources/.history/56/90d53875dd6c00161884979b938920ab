import java.util.Scanner;


public class SherlockandWatsonGymSecrets {

	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		int T = s.nextInt();
		while(T-- != 0)
		{
			int A = s.nextInt();
			int B = s.nextInt();
			int N = s.nextInt();
			int K = s.nextInt();
		}

	}

}
